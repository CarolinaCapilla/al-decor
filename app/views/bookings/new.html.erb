<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8">

      <h3>Select your pick!</h3>
      <%= simple_form_for [item, booking] do |f| %>
        <%= f.input :start_date %>
        <%= f.input :end_date %>
        <%= f.input :message %>
        <%= f.button :submit, class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>

<%= link_to 'Back', root_path %>
